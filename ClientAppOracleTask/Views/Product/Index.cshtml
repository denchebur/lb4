@model IEnumerable<ClientAppOracleTask.Entities.Product>
@{
    ViewBag.Title = "List of products";
}

<table class="table">
    <tr><th>№</th><th>Product Name</th><th>Price</th><th>Quantity</th><th>Producer</th><th>Producer Id</th></tr>
    @foreach (var product in Model)
    {
        <tr>
            <td>@product.Id</td>
            <td>@product.ProductName</td>
            <td>@product.Price</td>
            <td>@product.Quantity</td>
            <td>@product.Producer</td>
            <td>@product.DepartmentId</td>
        </tr>
    }
</table>